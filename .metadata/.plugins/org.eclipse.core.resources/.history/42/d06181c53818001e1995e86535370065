package com.practice;


import java.io.PrintWriter;

import javax.servlet.http.HttpServlet;
import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;

public class servlet2 extends HttpServlet {
	public void doGet(HttpServletRequest req, HttpServletResponse res)  {
		try {
			String studentName = req.getParameter("studetn_name");
			String studentEmail = req.getParameter("studetn_email");
			String studentPassword = req.getParameter("studetn_password");
			String studentGender = req.getParameter("studetn_gender");
			String studentCourse = req.getParameter("student_course");
			System.out.println(studentName + " " + studentEmail + " " + studentPassword + " " + studentGender + " " + studentCourse);

			PrintWriter writer = res.getWriter();
			writer.println("<h2> Your Name is : " + studentName + "</h2><br>" +
							"<h2> Your Name is : " + studentEmail+ "</h2><br>" +
							"<h2> Your Name is : " + studentPassword + "</h2><br>" +
							"<h2> Your Name is : " + studentGender+ "</h2><br>" +
							"<h2> Your Name is : " + studentCourse+ "</h2><br>");
		}catch (Exception e) {
			e.printStackTrace();
		}
		
	}
}
